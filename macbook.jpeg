        $(document).on('click', '.spf-product__swatch', function () {
            var product_id = parseInt($(this).data('product_id'));
            var regular_price_currency = $(this).data('regularprice');
            var compare_price_currency = $(this).data('compareprice');
            var a = regular_price_currency.trim();
            var currency = a.replace(/[\d\.,]/g, 'x').toString();
            var currency1 = a.replace(/[\d\.,a-zA-Z]/g, 'y').toString();
            var regularPrice = parseInt($(this).data('regular'));
            var offertext = $(this).data('offertext').replace(/\s+$/, '');
            var offer_price_text = offertext+":";
            var pricwithoutvat = document.getElementById("vat-"+product_id);
             var monthlyprice = document.getElementById("price_mon_section-"+product_id);
           if(vat_free_day == true){
              var vatDisPercent = parseFloat(vat_discount_amount)/100;
              var vatAmount1 = parseInt(regularPrice) * parseFloat(vatDisPercent);
              var vatAmount2 = parseInt(regularPrice) - parseFloat(vatAmount1);
              var am1 = parseFloat(vatAmount2, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString();
              var am2 = currency.replace('x',am1);
              var am3 = am2.replace(/x/g,'');
              $("#vat-"+product_id).html(am3);
              if(monthlyprice){
              var monthValue = monthlyprice.getAttribute("value");
            var month = parseInt(monthValue);
                 if(pricwithoutvat){
            var currentVariant = am3.trim().replace(/\D/g,'');
                 }
                else if(regular_price_currency) {
               var currentVariant = regular_price_currency.trim().replace(/\D/g,'');
                }

            currentVariant = currentVariant.toString();
            currentVariant = currentVariant.slice(0, -2);
            currentVariant = parseInt(currentVariant);
            var am1 = (currentVariant / month).toFixed(2);
            var am2 = currency.replace('x',am1).replace(/x/g,'');
             $("#price_mon_section-"+product_id).html(am2);
            }
            }


            var compar = parseFloat($(this).data('compare'));
            if(isNaN(compar)) {
                var compar = 0;
             }
            if (compar != 0 && compar > regularPrice && compar != regularPrice){
              $("#compare-price1-"+product_id).html("");
              $("#compare-price-"+product_id).addClass("spf-product-card__oldprice");
               $(".rg-"+product_id).removeClass("reghed2");
               $(".rg-"+product_id).addClass("reghed1");
              $(".oldpr-"+product_id).removeClass("reghed2");
               $(".oldpr-"+product_id).addClass("reghed1");
              $("#offer-"+product_id).html(offer_price_text);
              var e = parseFloat($(this).data('compare')) - parseFloat($(this).data('regular'));
              var disperc = parseFloat(e) * 100;
              var disperc2 =  parseFloat(disperc) / parseFloat(compar) ;
              var dis_limit = 15;
              $("#compare-price1-"+product_id).html(regular_price_currency);
              $(".offer-price-notempty-"+product_id).css("display","inline");
              var height = $('.dynamin-height-'+product_id).get(0).style.height;

              if(height == "0px"){
                $('.vatdiscountcss').css("height","28px");
                $('.vatdiscountcss').css("margin-top","4px");
                $('.vatpricecss').css("margin-top","4px");
              }

              $("#compare-price-"+product_id).html(compare_price_currency);
              if(disperc2 < dis_limit){
                var am11 = parseFloat(e, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString();
                var am22 = currency1.replace('y',am11);
                var am33 = am22.replace(/y/g,'');
                $("#discount-"+product_id).html('-'+ am33);
              }else{
                $("#discount-"+product_id).html('-'+ parseFloat(disperc2, 10).toFixed(2).replace(/(\d)(?=(\d{3})+\.)/g, "$1,").toString() +'%');
              }
            }else{
             $("#discount-"+product_id).html("");
             $("#offer-"+product_id).html("");
             $("#compare-price1-"+product_id).html("");
             $("#compare-price-"+product_id).html(regular_price_currency);
             $("#compare-price-"+product_id).removeClass ("spf-product-card__oldprice");
              $(".rg-"+product_id).removeClass("reghed1");
              $(".rg-"+product_id).addClass("reghed2");
              $(".oldpr-"+product_id).removeClass("reghed1");
               $(".oldpr-"+product_id).addClass("reghed2");
            }
          });
            
