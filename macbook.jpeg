    $(document).ready(function() {
        $('#rivermodalbtn').click(function(e) {
            $('#overlay').show();
            $('.fancybox-loading2').show();
            e.preventDefault();
            $.ajax({
                url: document.getElementById('rivermodalbtn').href,
                type: 'GET',
                dataType: 'html',
                success: function(data) {
                    $('#overlay').hide();
                    $('.fancybox-loading2').hide();

                    var iframe = document.createElement('iframe');
                    iframe.classList.add('iframe');
                    iframe.setAttribute('id', 'iframe-popup');
                    iframe.style.width = '100%';
                    iframe.style.height = '100%';
                    var popupContainer = document.getElementById('popupContainer');
                    popupContainer.innerHTML = '';
                    popupContainer.appendChild(iframe);

                    // Set the iframe onload event before setting srcdoc
                    openLinkInNewTab(iframe);

                    iframe.srcdoc = data; // Load the dynamic content into the iframe

                    function delayCss() {
                        var targetDiv = document.getElementsByClassName("fancybox-button--close")[0];
                        if (targetDiv) {
                            targetDiv.classList.add("close-icon-css");
                        } else {
                            console.log("Element with class 'fancybox-button--close' not found.");
                        }
                    }

                    setTimeout(delayCss, 1000);

                    $.fancybox.open({
                        src: '#popupContainer',
                        type: 'inline',
                        smallBtn: false,
                        infocus: true,
                        autoFocus: false
                    });
                },
                error: function (xhr, status, error) {
                    console.error('Error fetching data via AJAX', error);
                    $('#overlay').hide();
                    $('.fancybox-loading2').hide();
                }
            });
        });
    });

  function openLinkInNewTab(iframe) {
      iframe.onload = function() {
          var iframeDocument = iframe.contentWindow.document;
          var links = iframeDocument.querySelectorAll('a');
            console.log(links)
          for (var i = 0; i < links.length; i++) {
              // Check if the link already has a target attribute
              if (!links[i].getAttribute('target')) {
                  // If not, add target="_blank"
                  links[i].setAttribute('target', '_parent');
              }

              links[i].addEventListener('click', function(event) {
                  event.preventDefault();
                  var href = this.getAttribute('href');

                  if (href) {
                      window.open(href, '_parent'); // Open the link in a new tab
                  }
              });
          }
      };
  }
</script>
